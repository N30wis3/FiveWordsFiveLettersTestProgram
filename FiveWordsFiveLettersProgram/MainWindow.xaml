<Window x:Class="FiveWordsFiveLettersProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FiveWordsFiveLettersProgram"
        mc:Ignorable="d"
        Title="Five Words, Five Letters" Height="450" Width="800">
    <Grid>
        <Button x:Name="StartButton" Content="Start" HorizontalAlignment="Center" Margin="0,118,0,0" VerticalAlignment="Top" Height="99" Width="268" Click="StartButton_Click"/>
        <ProgressBar x:Name="Progress" HorizontalAlignment="Center" Height="41" Margin="0,289,0,0" VerticalAlignment="Top" Width="268"/>
        <ComboBox x:Name="FileComboBox" HorizontalAlignment="Left" Margin="635,55,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="TestData" />
            <ComboBoxItem Content="BetaData" />
            <ComboBoxItem Content="AlphaData" />
        </ComboBox>
        <TextBlock x:Name="ResultsText" HorizontalAlignment="Left" Margin="10,350,0,0" VerticalAlignment="Top" Width="770" Height="80" TextWrapping="Wrap" Text="Results will appear here."/>
    </Grid>
</Window>
